<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="sqlField_color">
	<select id="getSl" resultType="color.model.ColorBean">
		select * from color where col_name = '봄 라이트'
	</select>
	<select id="getSb" resultType="color.model.ColorBean">
		select * from color where col_name = '봄 브라이트'
	</select>
	
</mapper>